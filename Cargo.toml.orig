[package]
name = "mame_gui"
version = "0.1.0"
edition = "2021"

[dependencies]
eframe = "0.31.1"
egui_extras = { version = "0.31.1", features = ["image"] }
egui_glow = "0.31.1"
rfd = "0.13"
walkdir = "2.5"
quick-xml = { version = "0.31", features = ["serialize"] }
serde = { version = "1.0", features = ["derive"] }
serde_derive = "1.0"
serde_json = "1.0"
dirs = "5.0"
image = { version = "0.24.3", default-features = false, features = ["png"] } # Add this line
rayon = "1.7"
tokio = { version = "1", features = ["full"] }
zip = "0.6"
winit = "0.30.11"
